html {
  box-sizing: border-box;
}
*,
*::before,
*::after {
  box-sizing: inherit;
}
body {
  font-family: Arial, Helvetica, sans-serif;
  font-weight: 400;
  font-size: 17px;
  white-space: nowrap;
  color: rgba(0, 0, 0, 1);
  background: url(f09af602e12d7055428c866e61dba3d0) no-repeat;
  background-size: cover;
}
.wrapper {
  width: 1400px;
  margin: 0 auto;
  max-width: 100%;
  padding: 0 10px 50px 10px;
}
/* slyles header */
.header {
  padding: 20px;
  display: flex;
  justify-content:space-around;
  align-items: center;
}
.logo {
  display: inline-block;
}
.logo a {
  font-size: 30px;
  font-weight: 700;
  color: gray;
  text-decoration: none;
}
.logo:hover a {
  color: honeydew;
  transition: color 0.3s ease;
}
.icon-users{
  background: gray;
  color: antiquewhite;
  padding: 10px 30px;
  border-radius: 15px;
}
.icon-users:hover{
  cursor: pointer;
  background: antiquewhite;
  color: gray;
  transition: color 0.3s ease, background 0.3s ease;
}