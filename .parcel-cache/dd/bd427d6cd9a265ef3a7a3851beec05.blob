@font-face {
  font-family: 'icomoon';
  src:
    url('7584e8af2e7e14418ca210972d270952') format('truetype'),
    url('05e87c116e5c6cad3dd6c70c93889d6f') format('woff'),
    url('b24be32df1cb57fdbbe276865a130e5f') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="ic-"], [class*=" ic-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
